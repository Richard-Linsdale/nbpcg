<?xml version="1.0" encoding="UTF-8"?>
    <!DOCTYPE nbpcg-build PUBLIC "-//uk/theretiredprogrammer/nbpcgbuild/DTD NBPCGBUILD SCHEMA 1.0//EN" "nbres:/uk/theretiredprogrammer/nbpcg/nbpcgbuild.dtd">
<nbpcg-build name="Access Control Manager" copyright="2017 Richard Linsdale (richard at theretiredprogrammer.uk)">
    <folder project="Access Control Manager - Business Logic" location="java" package="uk.theretiredprogrammer.accesscontrolmanager.dataobjects" message="generating entity files" license="apache20">
        <execute template="entity" filename="User.java" useentityinfo="User"/>
        <execute template="entity" filename="Claim.java" useentityinfo="Claim"/>
        <execute template="rules" filename="UserRules.java" useentityinfo="User"/>
        <execute template="rules" filename="ClaimRules.java" useentityinfo="Claim"/>
        <execute template="restcreator" filename="AuthRestCreator.java" usepersistencestore="auth"/>
    </folder>
    <folder project="Access Control Manager - Business Logic" location="java" package="uk.theretiredprogrammer.accesscontrolmanager.dataobjects" message="generating dataaccess files" license="apache20">
        <execute template="htmlrest" filename="UserRest.java" useentityinfo="User" usepersistencestore="auth"/>
        <execute template="htmlrest" filename="ClaimRest.java" useentityinfo="Claim" usepersistencestore="auth"/>
    </folder>
    <folder project="Access Control Manager - Business Logic" location="java" package="uk.theretiredprogrammer.accesscontrolmanager.nodes" message="generating node files" license="apache20" datapackage="uk.theretiredprogrammer.accesscontrolmanager.dataobjects" editorpackage="uk.theretiredprogrammer.accesscontrolmanager.topcomponents">
        <execute template="rootnode" type="" filename="UserRootNode.java" useentityinfo="UserRoot"/>
        <execute template="node" type="" filename="UserNode.java" useentityinfo="User"/>
        <execute template="node" type="" filename="ClaimNode.java" useentityinfo="Claim"/>
        <execute template="nodefactory" type="" filename="UserRootNodeChildFactory.java" useentityinfo="UserRoot"/>
        <execute template="nodefactory" type="" filename="UserNodeChildFactory.java" useentityinfo="User"/>
        <execute template="undoaction" filename="UndoUser.java" useentityinfo="User"/>
        <execute template="undoaction" filename="UndoClaim.java" useentityinfo="Claim"/>
        <execute template="addaction" filename="AddUser.java" useentityinfo="User"/>
        <execute template="addaction" filename="AddClaim.java" useentityinfo="Claim"/>
        <execute template="children" filename="Users.java" useentityinfo="User"/>
        <execute template="children" filename="Claims.java" useentityinfo="Claim"/>
    </folder>
    <folder project="Access Control Manager - Business Logic" location="java" package="uk.theretiredprogrammer.accesscontrolmanager.topcomponents" message="generating node viewer files" license="apache20" datapackage="uk.theretiredprogrammer.accesscontrolmanager.dataobjects" nodepackage="uk.theretiredprogrammer.accesscontrolmanager.nodes">
        <execute template="rootnodeviewer" type="Tree" filename="UserRootNodeViewer.java" useentityinfo="UserRoot"/>
    </folder>
    <folder project="Access Control Manager - Business Logic" location="java" package="uk.theretiredprogrammer.accesscontrolmanager.topcomponents" message="generating editor files" license="apache20" datapackage="uk.theretiredprogrammer.accesscontrolmanager.dataobjects" nodepackage="uk.theretiredprogrammer.accesscontrolmanager.nodes">
        <execute template="editor" filename="UserEditor.java" useentityinfo="User"/>
        <execute template="editor" filename="ClaimEditor.java" useentityinfo="Claim"/>
        <execute template="editaction" filename="EditUser.java" useentityinfo="User"/>
        <execute template="editaction" filename="EditClaim.java" useentityinfo="Claim"/>
        <execute template="presenter" filename="UserPresenter.java" useentityinfo="User"/>
        <execute template="presenter" filename="ClaimPresenter.java" useentityinfo="Claim"/>
    </folder>
    <folder project="Access Control Manager - Business Logic" location="resource" package="uk.theretiredprogrammer.accesscontrolmanager.scripts.auth" message="generating script files for database" license="apache20"/>
</nbpcg-build>
    
        
        
    
    
        
            
                
                
                
                
                
                    
                    
                    
                    
                
            
            
                
                
                
                    
                    
                    
                
            
        
    
