<?xml version="1.0" encoding="UTF-8"?>
    <!DOCTYPE nbpcg-build PUBLIC "-//uk/org/rlinsdale/nbpcgbuild/DTD NBPCGBUILD SCHEMA 1.0//EN" "nbres:/uk/org/rlinsdale/nbpcg/nbpcgbuild.dtd">
<nbpcg-build name="Authentication" copyright="2014-2015 Richard Linsdale (richard.linsdale at blueyonder.co.uk)">
    <folder project="Lindos LifeCycle" location="resource" package="uk.org.rlinsdale.lindos.lifecycle.mysql.authentication2" message="generating mysql script files for authentication2" log="lindos.lifecycle" license="lgpl21">
        <execute template="createsqldatabase" filename="createdb.sql" usepersistencestore="authentication2"/>
        <execute template="backupscript" filename="backupscript.sh" usepersistencestore="authentication2"/>
        <execute template="createsqltables" filename="createtables.sql" usepersistencestore="authentication2"/>
    </folder>
    <folder project="Lindos LifeCycle" location="resource" package="uk.org.rlinsdale.lindos.lifecycle.json.authentication2" message="generating json persistence files for authentication2" log="lindos.lifecycle" license="lgpl21">
        <execute template="createjsontable" filename="User" usepersistencestore="authentication2" useentity="User"/>
        <execute template="createjsontable" filename="Application" usepersistencestore="authentication2" useentity="Application"/>
        <execute template="createjsontable" filename="Role" usepersistencestore="authentication2" useentity="Role"/>
        <execute template="createjsontable" filename="Permission" usepersistencestore="authentication2" useentity="Permission"/>
        <execute template="createjsontable" filename="Userrole" usepersistencestore="authentication2" useentity="Userrole"/>
        <execute template="createjsontable" filename="Userpermission" usepersistencestore="authentication2" useentity="Userpermission"/>
    </folder>
    <folder project="Lindos LifeCycle" location="java" package="uk.org.rlinsdale.lindos.authentication.dataobjects" message="generating entity files" log="lindos.lifecycle" license="lgpl21">
        <execute template="rootentity" filename="UserRoot.java" useentityinfo="UserRoot"/>
        <execute template="rootentity" filename="ApplicationRoot.java" useentityinfo="ApplicationRoot"/>
        <execute template="entity" filename="User.java" useentityinfo="User"/>
        <execute template="entity" filename="Application.java" useentityinfo="Application"/>
        <execute template="entity" filename="Role.java" useentityinfo="Role"/>
        <execute template="entity" filename="Permission.java" useentityinfo="Permission"/>
        <execute template="entity" filename="Userrole.java" useentityinfo="Userrole"/>
        <execute template="entity" filename="Userpermission.java" useentityinfo="Userpermission"/>
    </folder>
    <folder project="Access Control Manager - generated" location="java" package="uk.org.rlinsdale.accesscontrolmanager.nodes" message="generating node files" log="accesscontrolmanager" license="gpl30" datapackage="uk.org.rlinsdale.lindos.authentication.dataobjects">
        <execute template="rootnode" type="" filename="UserRootNode.java" useentityinfo="UserRoot"/>
        <execute template="rootnode" type="" filename="ApplicationRootNode.java" useentityinfo="ApplicationRoot"/>
        <execute template="node" type="" filename="UserNode.java" useentityinfo="User"/>
        <execute template="node" type="" filename="ApplicationNode.java" useentityinfo="Application"/>
        <execute template="node" type="" filename="UserpermissionNode.java" useentityinfo="Userpermission"/>
        <execute template="node" type="" filename="UserroleNode.java" useentityinfo="Userrole"/>
        <execute template="node" type="" filename="RoleNode.java" useentityinfo="Role"/>
        <execute template="node" type="" filename="PermissionNode.java" useentityinfo="Permission"/>
        <execute template="nodefactory" type="" filename="UserRootNodeChildFactory.java" useentityinfo="UserRoot"/>
        <execute template="nodefactory" type="" filename="UserNodeChildFactory.java" useentityinfo="User"/>
        <execute template="nodefactory" type="" filename="ApplicationRootNodeChildFactory.java" useentityinfo="ApplicationRoot"/>
        <execute template="nodefactory" type="" filename="ApplicationNodeChildFactory.java" useentityinfo="Application"/>
        <execute template="undonode" filename="UndoUserNode.java" useentityinfo="User"/>
        <execute template="undonode" filename="UndoUserpermissionNode.java" useentityinfo="Userpermission"/>
        <execute template="undonode" filename="UndoUserroleNode.java" useentityinfo="Userrole"/>
        <execute template="undonode" filename="UndoApplicationNode.java" useentityinfo="Application"/>
        <execute template="undonode" filename="UndoRoleNode.java" useentityinfo="Role"/>
        <execute template="undonode" filename="UndoPermissionNode.java" useentityinfo="Permission"/>
        <execute template="addnode" filename="AddUserNode.java" useentityinfo="User"/>
        <execute template="addnode" filename="AddUserpermissionNode.java" useentityinfo="Userpermission"/>
        <execute template="addnode" filename="AddUserroleNode.java" useentityinfo="Userrole"/>
        <execute template="addnode" filename="AddApplicationNode.java" useentityinfo="Application"/>
        <execute template="addnode" filename="AddRoleNode.java" useentityinfo="Role"/>
        <execute template="addnode" filename="AddPermissionNode.java" useentityinfo="Permission"/>
    </folder>
    <folder project="Access Control Manager - generated" location="java" package="uk.org.rlinsdale.accesscontrolmanager.topcomponents" message="generating node viewer files" log="accesscontrolmanager" license="gpl30" datapackage="uk.org.rlinsdale.lindos.authentication.dataobjects" nodepackage="uk.org.rlinsdale.accesscontrolmanager.nodes">
        <execute template="rootnodeviewer" type="Tree" filename="UserRootNodeViewer.java" useentityinfo="UserRoot"/>
        <execute template="rootnodeviewer" type="Tree" filename="ApplicationRootNodeViewer.java" useentityinfo="ApplicationRoot"/>
    </folder>
    <folder project="Access Control Manager - generated" location="java" package="uk.org.rlinsdale.accesscontrolmanager.topcomponents" message="generating node editor files" log="accesscontrolmanager" license="gpl30" datapackage="uk.org.rlinsdale.lindos.authentication.dataobjects" nodepackage="uk.org.rlinsdale.accesscontrolmanager.nodes">
        <execute template="nodeeditor" filename="UserNodeEditor.java" useentityinfo="User"/>
        <execute template="nodeeditor" filename="ApplicationNodeEditor.java" useentityinfo="Application"/>
        <execute template="nodeeditor" filename="RoleNodeEditor.java" useentityinfo="Role"/>
        <execute template="nodeeditor" filename="PermissionNodeEditor.java" useentityinfo="Permission"/>
        <execute template="nodeeditor" filename="UserroleNodeEditor.java" useentityinfo="Userrole"/>
        <execute template="nodeeditor" filename="UserpermissionNodeEditor.java" useentityinfo="Userpermission"/>
        <execute template="editnode" filename="EditUserNode.java" useentityinfo="User"/>
        <execute template="editnode" filename="EditUserpermissionNode.java" useentityinfo="Userpermission"/>
        <execute template="editnode" filename="EditUserroleNode.java" useentityinfo="Userrole"/>
        <execute template="editnode" filename="EditApplicationNode.java" useentityinfo="Application"/>
        <execute template="editnode" filename="EditRoleNode.java" useentityinfo="Role"/>
        <execute template="editnode" filename="EditPermissionNode.java" useentityinfo="Permission"/>
        <execute template="choice" filename="RoleChoiceField.java" useentityinfo="Role"/>
        <execute template="choice" filename="PermissionChoiceField.java" useentityinfo="Permission"/>
    </folder>
    <folder project="Authentication2" location="java" package="uk.org.rlinsdale.authentication2" message="generating remote files" log="authentication2" license="gpl30">
        <execute template="remoteentity" filename="User.java" usepersistencestore="authentication2" useentity="User"/>
        <execute template="remoteentity" filename="Application.java" usepersistencestore="authentication2" useentity="Application"/>
        <execute template="remoteentity" filename="Role.java" usepersistencestore="authentication2" useentity="Role"/>
        <execute template="remoteentity" filename="Permission.java" usepersistencestore="authentication2" useentity="Permission"/>
        <execute template="remoteentity" filename="Userrole.java" usepersistencestore="authentication2" useentity="Userrole"/>
        <execute template="remoteentity" filename="Userpermission.java" usepersistencestore="authentication2" useentity="Userpermission"/>
        <execute template="remotecommandprocessor" filename="UserCommandProcessor.java" usepersistencestore="authentication2" useentity="User"/>
        <execute template="remotecommandprocessor" filename="ApplicationCommandProcessor.java" usepersistencestore="authentication2" useentity="Application"/>
        <execute template="remotecommandprocessor" filename="RoleCommandProcessor.java" usepersistencestore="authentication2" useentity="Role"/>
        <execute template="remotecommandprocessor" filename="PermissionCommandProcessor.java" usepersistencestore="authentication2" useentity="Permission"/>
        <execute template="remotecommandprocessor" filename="UserroleCommandProcessor.java" usepersistencestore="authentication2" useentity="Userrole"/>
        <execute template="remotecommandprocessor" filename="UserpermissionCommandProcessor.java" usepersistencestore="authentication2" useentity="Userpermission"/>
        <execute template="remoteservlet" filename="Authentication2Servlet.java" usepersistencestore="authentication2"/>
    </folder>
    <folder project="Authentication2" location="resource" package="uk.org.rlinsdale.authentication2.mysql.authentication2" message="generating mysql script files for authentication2" log="authentication2" license="gpl30">
        <execute template="createsqldatabase" filename="createdb.sql" usepersistencestore="authentication2"/>
        <execute template="backupscript" filename="backupscript.sh" usepersistencestore="authentication2"/>
        <execute template="createsqltables" filename="createtables.sql" usepersistencestore="authentication2"/>
    </folder>
</nbpcg-build>
    
        
        
    
    
        
        
    
    
        
            
                
                
                
                
                
                    
                    
                    
                    
                
            
            
                
                
                    
                
                
                    
                
                
                    
                
            
            
                
                
                    
                    
                
                
                    
                    
                
                
                    
                    
                
            
            
                
            
            
                
                
                    
                    
                
                
                    
                    
                
                
                    
                    
                
            
            
                
            
        
    
