        <execute template="rootnodeviewer" type="Icon" filename="AlbumRootIconNodeViewer.java" useentityinfo="AlbumRoot"/>
        <execute template="iconnodeviewer" filename="ImageIconNodeViewer.java" useentityinfo="Image"/>
    </folder>
    <folder project="Photo Gallery - generated" location="java" package="uk.org.rlinsdale.photogallery.topcomponents" message="generating node editor files" log="linsdale.photogallery" license="gpl30" datapackage="uk.org.rlinsdale.photogallery.dataobjects" nodepackage="uk.org.rlinsdale.photogallery.nodes">
