    <!DOCTYPE nbpcg-build PUBLIC "-//uk/org/rlinsdale/nbpcgbuild/DTD NBPCGBUILD SCHEMA 1.0//EN" "nbres:/uk/org/rlinsdale/nbpcg/nbpcgbuild.dtd">
<nbpcg-build name="Photogallery" copyright="2015 Richard Linsdale (richard.linsdale at blueyonder.co.uk)">
    <folder project="Photo Gallery - generated" location="resource" package="uk.org.rlinsdale.photogallery.mysql" message="generating mysql script files for photogallery2" log="linsdale.photogallery" license="gpl30">
        <execute template="createsqldatabase" filename="createdb.sql" usepersistencestore="photogallery"/>
        <execute template="createsqldatabase" filename="createdb.sql" usepersistencestore="authentication"/>
        <execute template="backupscript" filename="backupscript.sh" usepersistencestore="photogallery"/>
        <execute template="backupscript" filename="backupscript.sh" usepersistencestore="authentication"/>
        <execute template="createsqltables" filename="createtables.sql" usepersistencestore="photogallery"/>
        <execute template="createsqltables" filename="createtables.sql" usepersistencestore="authentication"/>
    </folder>
    <folder project="Photo Gallery - generated" location="resource" package="uk.org.rlinsdale.photogallery.mysql" message="generating mysql script files for authentication2" log="linsdale.photogallery" license="gpl30">
        <execute template="createsqldatabase" filename="createdb.sql" usepersistencestore="photogallery"/>
        <execute template="createsqldatabase" filename="createdb.sql" usepersistencestore="authentication"/>
        <execute template="backupscript" filename="backupscript.sh" usepersistencestore="photogallery"/>
        <execute template="backupscript" filename="backupscript.sh" usepersistencestore="authentication"/>
        <execute template="createsqltables" filename="createtables.sql" usepersistencestore="photogallery"/>
        <execute template="createsqltables" filename="createtables.sql" usepersistencestore="authentication"/>
    </folder>
    <folder project="Photo Gallery - generated" location="java" package="uk.org.rlinsdale.photogallery.dataobjects" message="generating entity files" log="linsdale.photogallery" license="gpl30">
