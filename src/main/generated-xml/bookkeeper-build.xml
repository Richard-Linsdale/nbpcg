<?xml version="1.0" encoding="UTF-8"?>
    
    <!DOCTYPE nbpcg-build PUBLIC "-//uk/org/rlinsdale/nbpcgbuild/DTD NBPCGBUILD SCHEMA 1.0//EN" "nbres:/uk/org/rlinsdale/nbpcg/nbpcgbuild.dtd">
<nbpcg-build name="Book-Keeper" copyright="2015-2016 Richard Linsdale (richard.linsdale at blueyonder.co.uk)">
    <folder project="Book-Keeper - generated" location="resource" package="uk.org.rlinsdale.bookkeeper.mysql" message="generating mysql script files for bookkeeper" log="linsdale.bookkeeper" license="gpl30">
        <execute template="createsqldatabase" filename="createdb.sql" usepersistencestore="bookkeeper"/>
        <execute template="backupscript" filename="backupscript.sh" usepersistencestore="bookkeeper"/>
        <execute template="createsqltables" filename="createtables.sql" usepersistencestore="bookkeeper"/>
    </folder>
    <folder project="Book-Keeper - generated" location="java" package="uk.org.rlinsdale.bookkeeper.dataobjects" message="generating entity files" log="linsdale.bookkeeper" license="gpl30">
        <execute template="rootentity" filename="AccountRoot.java" useentityinfo="AccountRoot"/>
        <execute template="rootentity" filename="ProviderRoot.java" useentityinfo="ProviderRoot"/>
        <execute template="rootentity" filename="AssetCentreRoot.java" useentityinfo="AssetCentreRoot"/>
        <execute template="entity" filename="Account.java" useentityinfo="Account"/>
        <execute template="entity" filename="Provider.java" useentityinfo="Provider"/>
        <execute template="entity" filename="ProviderAccount.java" useentityinfo="ProviderAccount"/>
        <execute template="entity" filename="AssetCentre.java" useentityinfo="AssetCentre"/>
        <execute template="entity" filename="ProfitCentre.java" useentityinfo="ProfitCentre"/>
        <execute template="entity" filename="CostCentre.java" useentityinfo="CostCentre"/>
        <execute template="entity" filename="Journal.java" useentityinfo="Journal"/>
        <execute template="entity" filename="JournalEntry.java" useentityinfo="JournalEntry"/>
    </folder>
    <folder project="Book-Keeper - generated" location="java" package="uk.org.rlinsdale.bookkeeper.nodes" message="generating node files" log="linsdale.bookkeeper" license="gpl30" datapackage="uk.org.rlinsdale.bookkeeper.dataobjects" nodeeditorpackage="uk.org.rlinsdale.bookkeeper.topcomponents">
        <execute template="rootnode" type="" filename="AccountRootNode.java" useentityinfo="AccountRoot"/>
        <execute template="rootnode" type="" filename="ProviderRootNode.java" useentityinfo="ProviderRoot"/>
        <execute template="rootnode" type="" filename="AssetCentreRootNode.java" useentityinfo="AssetCentreRoot"/>
        <execute template="node" type="" filename="AccountNode.java" useentityinfo="Account"/>
        <execute template="node" type="" filename="ProviderNode.java" useentityinfo="Provider"/>
        <execute template="node" type="" filename="AssetCentreNode.java" useentityinfo="AssetCentre"/>
        <execute template="node" type="" filename="ProviderAccountNode.java" useentityinfo="ProviderAccount"/>
        <execute template="node" type="" filename="ProfitCentreNode.java" useentityinfo="ProfitCentre"/>
        <execute template="node" type="" filename="CostCentreNode.java" useentityinfo="CostCentre"/>
        <execute template="node" type="" filename="JournalNode.java" useentityinfo="Journal"/>
        <execute template="node" type="" filename="JournalEntryNode.java" useentityinfo="JournalEntry"/>
        <execute template="nodefactory" type="" filename="AccountRootNodeChildFactory.java" useentityinfo="AccountRoot"/>
        <execute template="nodefactory" type="" filename="ProviderRootNodeChildFactory.java" useentityinfo="ProviderRoot"/>
        <execute template="nodefactory" type="" filename="ProviderNodeChildFactory.java" useentityinfo="Provider"/>
        <execute template="nodefactory" type="" filename="AssetCentreRootNodeChildFactory.java" useentityinfo="AssetCentreRoot"/>
        <execute template="nodefactory" type="" filename="AssetCentreNodeChildFactory.java" useentityinfo="AssetCentre"/>
        <execute template="nodefactory" type="" filename="ProfitCentreNodeChildFactory.java" useentityinfo="ProfitCentre"/>
        <execute template="nodefactory" type="" filename="JournalNodeChildFactory.java" useentityinfo="Journal"/>
        <execute template="undonode" filename="UndoAccountNode.java" useentityinfo="Account"/>
        <execute template="undonode" filename="UndoProviderNode.java" useentityinfo="Provider"/>
        <execute template="undonode" filename="UndoProviderAccountNode.java" useentityinfo="ProviderAccount"/>
        <execute template="undonode" filename="UndoAssetCentreNode.java" useentityinfo="AssetCentre"/>
        <execute template="undonode" filename="UndoProfitCentreNode.java" useentityinfo="ProfitCentre"/>
        <execute template="undonode" filename="UndoCostCentreNode.java" useentityinfo="CostCentre"/>
        <execute template="undonode" filename="UndoJournalNode.java" useentityinfo="Journal"/>
        <execute template="undonode" filename="UndoJournalEntryNode.java" useentityinfo="JournalEntry"/>
        <execute template="addnode" filename="AddAccountNode.java" useentityinfo="Account"/>
        <execute template="addnode" filename="AddProviderNode.java" useentityinfo="Provider"/>
        <execute template="addnode" filename="AddProviderAccountNode.java" useentityinfo="ProviderAccount"/>
        <execute template="addnode" filename="AddAssetCentreNode.java" useentityinfo="AssetCentre"/>
        <execute template="addnode" filename="AddProfitCentreNode.java" useentityinfo="ProfitCentre"/>
        <execute template="addnode" filename="AddCostCentreNode.java" useentityinfo="CostCentre"/>
        <execute template="addnode" filename="AddJournalNode.java" useentityinfo="Journal"/>
        <execute template="addnode" filename="AddJournalEntryNode.java" useentityinfo="JournalEntry"/>
    </folder>
    <folder project="Book-Keeper - generated" location="java" package="uk.org.rlinsdale.bookkeeper.topcomponents" message="generating node viewer files" log="linsdale.bookkeeper" license="gpl30" datapackage="uk.org.rlinsdale.bookkeeper.dataobjects" nodepackage="uk.org.rlinsdale.bookkeeper.nodes">
        <execute template="rootnodeviewer" type="Tree" filename="AccountRootNodeViewer.java" useentityinfo="AccountRoot"/>
        <execute template="rootnodeviewer" type="Tree" filename="ProviderRootNodeViewer.java" useentityinfo="ProviderRoot"/>
        <execute template="rootnodeviewer" type="Tree" filename="AssetCentreRootNodeViewer.java" useentityinfo="AssetCentreRoot"/>
    </folder>
    <folder project="Book-Keeper - generated" location="java" package="uk.org.rlinsdale.bookkeeper.topcomponents" message="generating node editor files" log="linsdale.bookkeeper" license="gpl30" datapackage="uk.org.rlinsdale.bookkeeper.dataobjects" nodepackage="uk.org.rlinsdale.bookkeeper.nodes">
        <execute template="nodeeditor" filename="AccountNodeEditor.java" useentityinfo="Account"/>
        <execute template="nodeeditor" filename="ProviderNodeEditor.java" useentityinfo="Provider"/>
        <execute template="nodeeditor" filename="ProviderAccountNodeEditor.java" useentityinfo="ProviderAccount"/>
        <execute template="nodeeditor" filename="AssetCentreNodeEditor.java" useentityinfo="AssetCentre"/>
        <execute template="nodeeditor" filename="ProfitCentreNodeEditor.java" useentityinfo="ProfitCentre"/>
        <execute template="nodeeditor" filename="CostCentreNodeEditor.java" useentityinfo="CostCentre"/>
        <execute template="nodeeditor" filename="JournalNodeEditor.java" useentityinfo="Journal"/>
        <execute template="nodeeditor" filename="JournalEntryNodeEditor.java" useentityinfo="JournalEntry"/>
        <execute template="editnode" filename="EditAccountNode.java" useentityinfo="Account"/>
        <execute template="editnode" filename="EditProviderNode.java" useentityinfo="Provider"/>
        <execute template="editnode" filename="EditProviderAccountNode.java" useentityinfo="ProviderAccount"/>
        <execute template="editnode" filename="EditAssetCentreNode.java" useentityinfo="AssetCentre"/>
        <execute template="editnode" filename="EditProfitCentreNode.java" useentityinfo="ProfitCentre"/>
        <execute template="editnode" filename="EditCostCentreNode.java" useentityinfo="CostCentre"/>
        <execute template="editnode" filename="EditJournalNode.java" useentityinfo="Journal"/>
        <execute template="editnode" filename="EditJournalEntryNode.java" useentityinfo="JournalEntry"/>
        <execute template="presenter" filename="AccountPresenter.java" useentityinfo="Account"/>
        <execute template="presenter" filename="ProviderPresenter.java" useentityinfo="Provider"/>
        <execute template="presenter" filename="ProviderAccountPresenter.java" useentityinfo="ProviderAccount"/>
        <execute template="presenter" filename="AssetCentrePresenter.java" useentityinfo="AssetCentre"/>
        <execute template="presenter" filename="ProfitCentrePresenter.java" useentityinfo="ProfitCentre"/>
        <execute template="presenter" filename="CostCentrePresenter.java" useentityinfo="CostCentre"/>
        <execute template="presenter" filename="JournalPresenter.java" useentityinfo="Journal"/>
        <execute template="presenter" filename="JournalEntryPresenter.java" useentityinfo="JournalEntry"/>
        <execute template="tablepresenter" filename="ProfitCentreTablePresenter.java" useentityinfo="ProfitCentre"/>
        <execute template="tablepresenter" filename="CostCentreTablePresenter.java" useentityinfo="CostCentre"/>
        <execute template="tablepresenter" filename="JournalEntryTablePresenter.java" useentityinfo="JournalEntry"/>
    </folder>
</nbpcg-build>
    
    
    
          
    
        
    
    
    
        
            
        
        
            
        
    
    
    
        
            
                
                
                
            
            
                
                
                
            
            
                
                
                
                
                
            
            
                
                
            
            
                
                
            
            
                
                
            
            
                
                
            
            
                
                
                
                
                
            
        
    
